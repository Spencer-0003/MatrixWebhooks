generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

model Webhook {
  id        Int      @id @default(autoincrement())
  token     String   @unique @default(cuid())
  roomId    String
  ownerId   String
  createdAt DateTime @default(now())
}
